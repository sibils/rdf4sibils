          :SibilsTerminology ; 
     rdfs:label "Drugbank"^^xsd:string ; 
     :version "v5.1.5"^^xsd:string ; 
     rdfs:comment "The Drugbank database is a freely accessible resource which includes more than 13,000 records (version 5.1.4, released 2019-07-20). It contains information on drugs and drug targets, synonyms and product names."^^xsd:string ; 
     rdfs:isDefinedBy <http://sibils.org/rdf> ; 
